#include <iostream>
using namespace std;

float calcularMedia(int* v) {
    int soma = 0;
    for (int i = 0; i < 10; i++) {
        soma += v[i];
    }
    return soma / 10.0;
}

int main() {
    int numeros[10];

    cout << "Digite 10 numeros:\n";
    for (int i = 0; i < 10; i++) {
        cin >> numeros[i];
    }

    float media = calcularMedia(numeros);

    cout << "Media = " << media << endl;

    return 0;
}
