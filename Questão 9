#include <iostream>
using namespace std;

// Função que calcula o quadrado de um número
int quadrado(int numero) {
    return numero * numero;
}

int main() {
    int valor;
    cout << "Digite um número: ";
    cin >> valor;

    cout << "O quadrado de " << valor << " é " << quadrado(valor) << endl;

    return 0;
}
