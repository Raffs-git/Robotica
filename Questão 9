#include <iostream>
using namespace std;

//Declaração de Função 
int quadrado(int numero) {
    return numero * numero;
}

int main() {
    int numero1;
    cout << "Digite um número: ";
    cin >> numero1;

    cout << "O quadrado de " << numero1 << " é " << quadrado(numero1) << endl;

    return 0;
}
